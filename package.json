{
  "name": "vercel-postgres-backend",
  "version": "1.0.0",
  "description": "Backend application with Express and PostgreSQL for Vercel deployment",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm migration:create",
    "migration:generate": "npm run typeorm migration:generate -d src/config/data-source.ts",
    "migration:run": "npm run typeorm migration:run -d src/config/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -d src/config/data-source.ts",
    "seed": "ts-node src/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@supabase/supabase-js": "^2.39.3",
    "@vercel/postgres": "^0.7.2",
    "@vercel/speed-insights": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.16",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "typeorm-ts-node-commonjs": "^0.3.20"
  }
}
